{
    "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "name": {
            "type": "string",
            "defaultValue": ""
        },
        "location": {
            "type": "string",
            "defaultValue": "[resourceGroup().location]"
        },
        "owner": {
            "type": "string",
            "defaultValue": "",
            "metadata": {
                "description": "A project owner's email address"
            }
        },
        "projectName": {
            "type": "string",
            "defaultValue": "",
            "metadata": {
                "description": "A project name"
            }
        },
        "projectCode": {
            "type": "string",
            "defaultValue": "",
            "metadata": {
                "description": "A project code"
            }
        },
        "addressPrefix": {
            "type": "string",
            "defaultValue": "",
            "metadata": {
                "description": "A primary address scope in this Virtual Networks"
            }
        }
    },
    "variables": {

    },
    "resources": [
        {
            "name": "01-devtest-labs-minimum",
            "type": "Microsoft.Resources/deployments",
            "apiVersion": "2019-05-01",
            "properties": {
                "mode": "Incremental",
                "templateLink": {
                    "uri": "https://raw.githubusercontent.com/0x6797/azure-rm-templates/master/devtest_labs/01-devtest-labs-minimum.json"
                },
                "parameters": {
                    "name": {
                        "value": "[parameters('name')]"
                    },
                    "location": {
                        "value": "[parameters('location')]"
                    },
                    "owner": {
                        "value": "[parameters('owner')]"
                    },
                    "projectName": {
                        "value": "[parameters('projectName')]"
                    },
                    "projectCode": {
                        "value": "[parameters('projectCode')]"
                    }
                }
            }
        },
        {
            "name": "02-vnet",
            "type": "Microsoft.Resources/deployments",
            "apiVersion": "2019-05-01",
            "dependsOn": [
                "01-devtest-labs-minimum"
            ],
            "properties": {
                "mode": "Incremental",
                "templateLink": {
                    "uri": "https://raw.githubusercontent.com/0x6797/azure-rm-templates/master/devtest_labs/02-vnet.json"
                },
                "parameters": {
                    "name": {
                        "value": "[parameters('name')]"
                    },
                    "location": {
                        "value": "[parameters('location')]"
                    },
                    "owner": {
                        "value": "[parameters('owner')]"
                    },
                    "projectName": {
                        "value": "[parameters('projectName')]"
                    },
                    "projectCode": {
                        "value": "[parameters('projectCode')]"
                    },
                    "devTestLabsId": {
                        "value": "[reference('01-devtest-labs-minimum').outputs.devTestLabsId.value]"
                    },
                    "devTestLabsUId": {
                        "value": "[reference('01-devtest-labs-minimum').outputs.devTestLabsUId.value]"
                    },
                    "addressPrefix": {
                        "value": "[parameters('addressPrefix')]"
                    }
                }
            }
        }
    ]
}